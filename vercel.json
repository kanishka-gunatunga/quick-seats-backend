{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "crons": [
    {
      "path": "/cron/reset-seats",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/cron/cleanup-pending-seats",
      "schedule": "*/5 * * * *"
    }
  ]
}
